<div class="col s12 m12">
  <div class="row">
    {{#each legend_first}}
      <div class="col s12 m4">
        <div class="row">
          <div class="col s6 m6">
            <svg height="50" width="50">
              <circle cx="25" cy="25" r="15" stroke="black" stroke-width="1" fill={{color}} />
            </svg>
          </div>
          <div class="col s6 m6">
            <p>{{name}}: {{pct}}</p>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
  <div class="row">
    {{#each legend_second}}
      <div class="col s12 m4">
        <div class="row">
          <div class="col s6 m6">
            <svg height="50" width="50">
              <circle cx="25" cy="25" r="15" stroke="black" stroke-width="1" fill={{color}} />
            </svg>
          </div>
          <div class="col s6 m6">
            <p>{{name}}: {{pct}}</p>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>
