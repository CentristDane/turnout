<!-- open primary container -->
<div class="container">
  {{> nav-partial}}
  <!-- primary row -->
  <div class="row">
    <!-- primary col -->
    <div class="col s12 m12">
      <!-- map row -->
      <div class="row">
        <div class="col s12 m12">
          <div class="leaflet">
            <!-- inset leaflet here -->
          </div>
          <div class="caption">
            <!-- insert caption info here -->
          </div>
        </div>
      </div> <!-- Close map row -->
      <!-- open button row -->
      <div class="row">
        <div class="col s12 m12">
          <!-- top row -->
          <div class="row">
            <div class="col s12 m4">
              <a class="waves-effect waves-light btn">Low Turnout</a>
            </div>
            <div class="col s12 m4">
              <a class="waves-effect waves-light btn">Medium Turnout</a>
            </div>
            <div class="col s12 m4">
              <a class="waves-effect waves-light btn">High Turnout</a>
            </div>
          </div>
          <!-- text row -->
          <div class="row">
            <p id="turnout-desc">Test text</p>
          </div>
        </div>
      </div> <!-- Close button row -->
      <!-- open dynamic info row -->
      <div class="row">
        <!-- info col -->
        <div class="col s12 m8">

        </div>
        <!-- button col -->
        <div class="col s12 m4">

        </div>
      </div> <!-- Close dynamic info row -->
    </div>
  </div>
</div> <!-- close primary container -->
